{
  "manifest_version": 3,
  "name": "Refreshment-extension",
  "version": "1.0",
  "description": "A chrome extension that helps you relax",
  "permissions": ["activeTab"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
